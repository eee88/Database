package 삼항연산자;

import java.util.Scanner;

public class Ex03농구공예제 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("농구공의 개수를 입력하세요 : ");
		int num = sc.nextInt();
		int vaulue = num/5;
		int result = (num%5==0) ? vaulue : vaulue + 1;
		System.out.println("필요한 상자의 개수 : " + result);
	}

}
